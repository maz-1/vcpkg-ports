{
  "name": "libcitygml",
  "version-semver": "2.4.1",
  "description": "C++ Library for CityGML Parsing and Visualization",
  "homepage": "https://github.com/jklimke/libcitygml",
  "supports": "windows",
  "dependencies": [
    "xerces-c"
  ],
  "features": {
    "gdal": {
      "description": "Enable coordinate transformations support",
      "dependencies": [
        "gdal"
      ]
    },
    "osg": {
      "description": "Enable OpenSceneGraph plugin",
      "dependencies": [
        "osg"
      ]
    },
    "tools": {
      "description": "Enable tools and tests"
    }
  }
}
